<%= render :partial => "partial/header" %>
<section>
  <div class="container">
    <ul class="task-list">
      <li class="panel panel-default">
        <div class="panel-heading">
          <h3><%= @task.title %></h3>
        </div>
        <div class="panel-body">
          <pre><%= @task.description %></pre>
          <hr>
          <%= link_to "Back", tasks_path, class: "btn btn-default" %>
